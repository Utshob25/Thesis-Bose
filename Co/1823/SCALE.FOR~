      PROGRAM DIFFUSION CONSTANT USING LEE AND DZTUV
CC    T IN K,DIFF IN E-09 m2/s, ETA IN mPa.s 
CC    NUMBER DENSITY (DENA) IN /ANGS**3.0
      IMPLICIT REAL*8 (A-H,O-Z)
      

      OPEN(5,FILE='DIFIN.DAT',STATUS='OLD')
      READ(5,*)SE,DENA
      T=1823.0
      ATM=55.85
      AKB=1.38
      SIG=2.48
      PCSIG=3.116
      PI=3.14159265358979
      COND=30.9529293013655
      CONE=0.0516913919332803
      
      ETASTR=0.035*EXP(-0.55*SE)
C      PRINT*,ETASTR
      ETAL=CONE*ETASTR*SIG*PCSIG*DENA*((PI*ATM*AKB*T)**0.50)
C      PRINT*,ETA

      DSTR=0.049*EXP(SE)
      
      DIFFD=COND*DSTR*(SIG**4.0)*PCSIG*DENA*((PI*AKB*T/ATM)**0.50)
      PRINT*,DIFFD,ETAL
      
      STOP
      END
C
